@model IList<CategoryModel>

@using Nop.Web.Models.Catalog;

@if (Model.Count > 0)
{
	<div class="container">
        @foreach (var item in Model)
        {
            <div class="element">
                <a href="@Url.RouteUrl("Category", new { SeName = item.SeName })">
                    <span>@item.Name</span>
                    <img src="@item.PictureModel.ImageUrl" alt="@item.PictureModel.AlternateText" />
                </a>
            </div>
        }
    </div>
}
